DOMAINS
	N,Y,N1,S2 = INTEGER
	
PREDICATES
	FINDSUM(INTEGER,INTEGER)
	
GOAL

	WRITE("ENTER THE NUMBER OF INTEGERS "),
	READINT(N),
	FINDSUM(N,0).
	
CLAUSES
	FINDSUM(0,S):-  WRITE("SUM = ",S).
	FINDSUM(N,S):-  WRITE("ENTER ELES : "),
			READINT(Y),
			S1=Y+S,
			N1=N-1,
			FINDSUM(N1,S1).
	